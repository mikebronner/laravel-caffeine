{
    "name": "genealabs/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/routing": "^6.0",
        "illuminate/support": "^6.0",
        "jenssegers/model": "^1.2.1"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "mockery/mockery": "^1.0",
        "orchestra/testbench-browser-kit": "^4.0",
        "orchestra/testbench-dusk": "^4.0",
        "orchestra/testbench": "^4.0",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls" : "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
